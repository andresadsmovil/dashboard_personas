view: logo {
  sql_table_name: `adsmovil-reports.looker_ml.logo` ;;

  dimension: account_id {
    type: number
    sql: ${TABLE}.account_id ;;
  }
  dimension: logo {
    type: string
    sql: ${TABLE}.logo ;;
    html: "<img src='{{ logo }}' style='max-width: 100px; max-height: 50px;'/>" ;;
  }
  measure: count {
    type: count
  }
  measure: dynamic_logo {
    type: string sql: CASE WHEN ${account_id} = 1 THEN 'https://drive.google.com/uc?export=view&id=1kcXzehdUUkaJyEtQF0nLW-UmW53herYG'
                      WHEN ${account_id} = 2 THEN 'data:image/png;base64,'
                      ELSE 'https://drive.google.com/uc?export=view&id=1kcXzehdUUkaJyEtQF0nLW-UmW53herYG' END ;;
    html: <img src="{{ value }}" alt="Logo"> ;; }
}
